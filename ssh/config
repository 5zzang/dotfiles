## Usage :
## Host hostName
##   HostName www.google.com
##   Port 22 (Optional)
##   User 5zzang
##   PreferredAuthentications publickey (Optional)
##   IdentityFile ~/.ssh/5zzang

# KKC
Host bastion.dev.kokkok.asia
  User tw.kim
  HostName bastion.dev.kokkok.asia
  Port 2222
  IdentityFile ~/.ssh/keys/kkc/tw.kim

Host nat.dev.lotto.kks.internal
  User ec2-user
  HostName nat.dev.lotto.kks.internal
  ProxyJump tw.kim@bastion.dev.kokkok.asia:2222
  IdentityFile ~/.ssh/keys/kkc/kk-lotto-dev-keypair.pem

# Laoodoo
Host bastion.kokkokmart.com
  User tw.kim
  HostName bastion.kokkokmart.com
  Port 2222
  IdentityFile ~/.ssh/keys/kkc/tw.kim

Host backoffice.prd.laoodoo.internal
  User ec2-user
  HostName backoffice.prd.laoodoo.internal
  ProxyJump tw.kim@bastion.kokkokmart.com:2222
  IdentityFile ~/.ssh/keys/kkc/laoodoo-prd-keypair.pem

Host fc-a.prd.laoodoo.internal
  User ec2-user
  HostName fc-a.prd.laoodoo.internal
  ProxyJump tw.kim@bastion.kokkokmart.com:2222
  IdentityFile ~/.ssh/keys/kkc/laoodoo-prd-keypair.pem

# github.com
Host github.com
    Hostname github.com
    IdentityFile ~/.ssh/keys/5zzang/5zzang
    IdentitiesOnly yes

Host github-kkc
    Hostname github.com
    User 5zzang-kkc
    IdentityFile ~/.ssh/keys/kkc/tw.kim
    IdentitiesOnly yes


